{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "*": ["types/*"]
    },
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.png",
    "src/sidebar/Popfinal.png.d.ts",
    "src/reportWebVitals.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
